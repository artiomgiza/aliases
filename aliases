ulimit -n 10000

getpod() {                       
    kubectl --namespace scrum"$1" get pod | grep "$2"- | grep -v redis| grep -v sphinx |  grep -v scheduler |  grep -v worker
}

getbash() {                      
 kubectl --namespace scrum"$1"  exec -it "$2" -c "$3" bash     
}



